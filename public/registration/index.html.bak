    <!-- ======= Book A Table Section ======= -->
    <section id="book-a-table" class="book-a-table">
      <div class="container" data-aos="fade-up">
        <form action="" method="post" role="form" class="php-email-form" data-aos="fade-up" data-aos-delay="100">
          <div class="form-row">
            <div class="col-lg-4 col-md-6 form-group">
              <div class="register-drowdown">
                <select id="programs" name="programs">
                  <option selected disabled hidden>Choose a Program</option>
                  <option value="mens over 18 soccer league">Men's 18+ Soccer League</option>
                  <option value="ramadan wellness program">Ramadan Wellness Program</option>
                  <option value="active club">Active (running, cycling, hiking)</option>
                </select>
              </div>
            </div>           
          </div>
        </form>

        <form id="infoForm" action='' method="post" role="form" class="php-email-form" data-aos="fade-up" data-aos-delay="100">
          <div class="form-row">
            <div class="col-lg-4 col-md-6 form-group">
              <label>First Name</label>
              <input type="text" name="firstName" class="form-control" id="firstName" placeholder="Your First Name" data-rule="minlen:1" data-msg="Please enter at least 1 char">
              <div class="validate"></div>
            </div>            
            <div class="col-lg-4 col-md-6 form-group">
              <label>Last Name</label>
              <input type="text" name="lastName" class="form-control" id="lastName" placeholder="Your Last Name" data-rule="minlen:1" data-msg="Please enter at least 1 chars">
              <div class="validate"></div>
            </div>
            <div class="col-lg-4 col-md-6 form-group">
              <label>Phone</label>
              <input type="text" class="form-control" name="phone" id="phone" placeholder="Your Phone" data-rule="minlen:10" data-msg="Please enter a valid phone number">
              <div class="validate"></div>
              </div>
            <div class="col-lg-4 col-md-6 form-group">
              <label>Email</label>
              <input type="email" class="form-control" name="email" id="email" placeholder="Your Email" data-rule="email" data-msg="Please enter a valid email">
              <div class="validate"></div>
            </div>
            <div class="col-lg-4 col-md-6 form-group">
              <label>Birthday</label>
              <input type="date" class="form-control" name="age" id="birthday" placeholder="Your Birthday" required min="1920-01-01" max='2021-01-01' data-msg="Please enter a valid birthday">
              <div class="validate"></div>
            </div>
            <div class="col-lg-4 col-md-6 form-group">
              <label>Gender</label>
                <div class="register-drowdown">
                    <select id="gender" name="gender" required>
                        <option selected disabled hidden value="">Your Gender</option>
                        <option value="Male">Male</option>
                        <option value="Female">Female</option>
                    </select>
                </div>
            </div>

          <div class="program-waivers form-group">
              <h5>Waiver</h5>
              <div class='waiver'>
                <iframe class="register-terms" src="../assets/waivers/soccerMensOver18.txt"></iframe>
              </div>
              <br>
              <input type="checkbox" id="terms" required name="terms">
              <label for="terms"> I accept the terms and conditions mentioned in the above waiver </label><br>  
            </div>
          </div>
          <div class="mb-3">
            <div class="loading">Loading</div>
            <div class="error-message"></div>
            <div class="sent-message">Your registration request was sent. We will send an Email to confirm your registration. Thank you!</div>
          </div>
          <div class="text-center"><button type="submit" onclick="validateProgram()">Register</button></div>
        </form>

      <div id="no-program">Registration for this program is currently closed. Contact <strong>info@maaweb.org</strong> for more information.</div>

      </div>
    </section><!-- End Book A Table Section -->

    <div class="checkout-payment" id="form-container">
      <h3>Checkout</h3>
      <hr></hr>
      <label class="checkout-label">Program: </label>
      <div id="program-name-checkout"></div>
      <label class="checkout-label">Price: </label><div id="program-price"></div>
      <hr></hr>
      <br></br>
      <div id="sq-card-number"></div>
      <div class="third" id="sq-expiration-date"></div>
      <div class="third" id="sq-cvv"></div>
      <div class="third" id="sq-postal-code"></div>
      <button id="sq-creditcard" class="button-credit-card" onclick="register(event)">Confirm Payment ($130)</button>
     </div>

